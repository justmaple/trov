  <TestCase value="precedes">
    <objective>less than zero: string precedes - StringA longer than StringB - Alpha upper lower cases

</objective>
    <stringA>moMents
</stringA>
    <indexA>2</indexA>
    <stringB>mament
</stringB>
    <indexB>2</indexB>
    <length>20</length>
