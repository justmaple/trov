  <TestCase value="ArgumentOutOfRangeExceptionCase&#xD;&#xA;">
    <objective>indexA is greater than stringA.length
</objective>
    <stringA>cat
</stringA>
    <indexA>4</indexA>
    <stringB>music</stringB>
    <indexB>1</indexB>
    <length>5</length>
    <expectedResult></expectedResult>
  </TestCase>
  <TestCase value="ArgumentOutOfRangeExceptionCase&#xD;&#xA;">
    <objective>indexB is greater than stringB.length
</objective>
    <stringA>dog
</stringA>
    <indexA>1</indexA>
    <stringB>woof
</stringB>
    <indexB>5</indexB>
    <length>5</length>
    <expectedResult></expectedResult>
  </TestCase>
